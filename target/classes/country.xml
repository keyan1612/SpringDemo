<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




<bean id="c1" class="com.lti.springdemo.Country">
<constructor-arg index="0" value="India"></constructor-arg>
<constructor-arg index="1" value="2000"> </constructor-arg>

</bean>
<bean id="s1" class="com.lti.springdemo.Softwares">
<property name="listofLang">
<list>
<value>Core Java</value>
<value>C#</value>
<value>Python</value>
<value>C++</value>
</list>
</property>
</bean>
</beans>